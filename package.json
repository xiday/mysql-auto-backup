{
  "name": "mysql-auto-backup",
  "version": "1.0.0",
  "description": "auto backup sql file to aliyun oss",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --inspect --watch src --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "server": "node dist/index.js",
    "init-docker": "rimraf docker/package.json docker/yarn.lock docker/dist && cp -r dist package.json yarn.lock docker",
    "lint": "eslint ./src"
  },
  "author": "MrFei",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "nodemon": "^1.19.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "ali-oss": "^6.1.1",
    "archiver": "^3.0.0",
    "cron": "^1.7.1",
    "fs-extra": "^8.0.1",
    "json5": "^2.1.3",
    "lodash": "^4.17.21",
    "log4js": "^4.3.0",
    "mysqldump": "^3.0.0"
  }
}
